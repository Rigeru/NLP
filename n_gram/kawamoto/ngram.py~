#!/usr/bin/env python
# -*- coding: utf-8 -*-

from sys import *
from collections import deque

#可変長引数
def ngram(*args, **kwargs):
    print (len(args))
    

    
    




if __name__ == "__main__":
    if len(argv) != 2:
        print("Usage: # python %s \"N\" " % argv[0])
        quit()
    N = argv[1]
    queue = deque('^')
    dict = {}
    for line in stdin:
        word = line.split()[0]
        if len(queue) == N:
            dict[queue] = dict.get(queue,0) + 1
            queue.popleft()
            queue.append(word)
        else:
            queue.append(word)
    dict[queue] = dict.get(queue,0) + 1
